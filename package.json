{
	"name": "digital-ocean-manager",
	"displayName": "digital-ocean-manager",
	"description": "Help you list/create/destroy droplets on VsCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:digital-ocean-manager.dropletCreate",
		"onCommand:digital-ocean-manager.dropletList",
		"onCommand:digital-ocean-manager.dropletDestroy"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "digital-ocean-manager.dropletCreate",
				"title": "Digital Ocean Manager: Droplet Create"
			},
			{
				"command": "digital-ocean-manager.dropletList",
				"title": "Digital Ocean Manager: Droplet List"
			},
			{
				"command": "digital-ocean-manager.dropletDestroy",
				"title": "Digital Ocean Manager: Droplet Destroy"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}